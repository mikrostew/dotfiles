# Re-enable the ssh-dss (DSA) public key algorithm
Host *

    HostkeyAlgorithms +ssh-dss
    PubkeyAcceptedKeyTypes +ssh-dss
    AddKeysToAgent yes
    UseKeychain yes
    # need this after upgrade to 10.13.3
    Ciphers aes128-cbc,3des-cbc,aes256-ctr

# tester account
Host *.corp.linkedin.com
    IdentityFile    %d/.ssh/tester_at_linkedin.com_rsa_key

# LinkedIn
Host *.linkedin.com
    IdentityFile 	%d/.ssh/%u_at_linkedin.com_ssh_key

Host desktop
    Hostname mistewar-ld2.linkedin.biz
    User mistewar
    IdentityFile    %d/.ssh/%u_at_linkedin.com_ssh_key

Host laptop
    Hostname mistewar-mn1.linkedin.biz
    User mistewar
    IdentityFile    %d/.ssh/%u_at_linkedin.com_ssh_key

# personal
Host github.com
    Hostname github.com
    User mikrostew
    # IdentityFile    ~/.ssh/github_mikrostew
    IdentityFile    %d/.ssh/github_rsa
    IdentitiesOnly yes

Host air
    Hostname Michaels-Air.attlocal.net
    User mikrostew

Host pi
    Hostname raspberrypi
    User pi

Host android
    Hostname 192.168.86.30
    Port 2222
    IdentityFile    %d/.ssh/android_rsa

Host android-tether
    Hostname 192.168.43.1
    Port 2222
    IdentityFile    %d/.ssh/android_rsa
