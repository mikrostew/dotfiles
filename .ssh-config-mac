# Re-enable the ssh-dss (DSA) public key algorithm
Host *

    HostkeyAlgorithms +ssh-dss
    PubkeyAcceptedKeyTypes +ssh-dss
    AddKeysToAgent yes
    UseKeychain yes

# tester account
Host *.corp.linkedin.com
    IdentityFile    %d/.ssh/tester_at_linkedin.com_rsa_key

# personal
Host github.com
    Hostname github.com
    User mikrostew
    # IdentityFile    ~/.ssh/github_mikrostew
    IdentityFile    ~/.ssh/github_rsa
    IdentitiesOnly yes

# LinkedIn
Host *.linkedin.com
	IdentityFile 	%d/.ssh/%u_at_linkedin.com_ssh_key

Host desktop
    Hostname mistewar-ld2.linkedin.biz
    User mistewar
    IdentityFile    %d/.ssh/%u_at_linkedin.com_ssh_key

Host laptop
    Hostname mistewar-mn1.linkedin.biz
    User mistewar
    IdentityFile    %d/.ssh/%u_at_linkedin.com_ssh_key

Host pi
    Hostname raspberrypi
    User pi

# # local VMs
